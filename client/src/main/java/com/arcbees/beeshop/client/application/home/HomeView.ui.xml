<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'
        xmlns:widget="urn:import:com.arcbees.beeshop.client.application.widget"
        xmlns:svg="urn:import:org.vectomatic.dom.svg.ui">

    <ui:with field="res" type="com.arcbees.beeshop.client.resources.AppResources"/>
    <ui:with field="comp" type="com.arcbees.beeshop.client.resources.ComponentsResources"/>
    <ui:with field="font" type="com.arcbees.beeshop.client.resources.FontResources"/>
    <ui:with field="page" type="com.arcbees.beeshop.client.resources.PageHomeResources"/>

    <g:HTMLPanel>

        <section class="{page.style.mainProducts}">
            <div class="{page.style.mainProducts_left} {res.style.mainProducts_bg}">
                <g:Image resource="{page.tshirt}"/>

                <widget:PricePresenter ui:field="shirtPrice"/>
            </div>

            <div class="{page.style.mainProducts_right}">
                <g:Image resource="{page.arcbeesBag}" addStyleNames="{res.style.arcbees_img}"/>
                <g:Image resource="{page.chosenBag}" addStyleNames="{res.style.chosen_img}"/>
                <g:Image resource="{page.gaeBag}" addStyleNames="{res.style.gaestudio_img}"/>
                <g:Image resource="{page.gqueryBag}" addStyleNames="{res.style.gquery_img}"/>
                <g:Image resource="{page.gsssBag}" addStyleNames="{res.style.gsss_img}"/>
                <g:Image resource="{page.gwtpBag}" addStyleNames="{res.style.gwtp_img}"/>
                <g:Image resource="{page.jukitoBag}" addStyleNames="{res.style.jukito_img}"/>

                <widget:PricePresenter ui:field="bagPrice"/>
            </div>
        </section>

        <section class="{res.style.stripe} {res.style.txt_center} {page.style.shoppingByProduct}">
            <h3>shopping by products</h3>
            <ul class="{page.style.devProducts_list}">
                <li>
                    <a ui:field="gaeAnchor">
                        <svg:SVGImage resource="{res.gae}"/>
                        <span class="{res.style.visuallyHidden}">GAE STUDIO</span>
                    </a>
                </li>
                <li>
                    <a ui:field="gwtpAnchor">
                        <svg:SVGImage resource="{res.gwtp}"/>
                        <span class="{res.style.visuallyHidden}">GWTP</span>
                    </a>
                </li>
                <li>
                    <a ui:field="jukitoAnchor">
                        <svg:SVGImage resource="{res.jukito}"/>
                        <span class="{res.style.visuallyHidden}">JUKITO</span>
                    </a>
                </li>
                <li>
                    <a ui:field="arcbeesAnchor">
                        <img src="data:image/svg+xml;base64,





"/>
                        <svg:SVGImage resource="{res.arcbees}"/>
                        <span class="{res.style.visuallyHidden}">arcbees</span>
                    </a>
                </li>
                <li>
                    <a ui:field="gqueryAnchor">
                        <object type="image/svg+xml" data="data:image/svg+xml;base64,





">
                            fallback
                        </object>
                        <svg:SVGImage resource="{res.gquery}"/>
                        <span class="{res.style.visuallyHidden}">gQuery</span>
                    </a>
                </li>
                <li>
                    <a ui:field="gsssAnchor">
                        <svg:SVGImage resource="{res.gsss}"/>
                        <span class="{res.style.visuallyHidden}">GSSS</span>
                    </a>
                </li>
                <li>
                    <a ui:field="chosenAnchor">
                        <svg:SVGImage resource="{res.chosen}"/>
                        <span class="{res.style.visuallyHidden}">GWT chosen</span>
                    </a>
                </li>
            </ul>
        </section>

        <section class="{res.style.stripe} {page.style.welcome}">
            <h2>Welcome to the Beestore</h2>
            <p>The Beestore is a study case from Arcbees
                <span>to show our hive of products and branded gears. Enjoy!</span>
            </p>
        </section>

        <section class="{page.style.itemsForSale}">
            <div class="{page.style.itemForSale}">
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.arcbees_img}" resource="{page.arcbeesThermos}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gwtp_img}" resource="{page.gwtpThermos}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gquery_img}" resource="{page.gqueryThermos}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gaestudio_img}" resource="{page.gaeThermos}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.chosen_img}" resource="{page.chosenThermos}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.jukito_img}" resource="{page.jukitoThermos}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gsss_img}" resource="{page.gsssThermos}"/>

                <widget:PricePresenter ui:field="cupPrice"/>
            </div>
            <div class="{page.style.itemForSale}">
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.arcbees_img}" resource="{page.arcbeesCase}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gwtp_img}" resource="{page.gwtpCase}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gquery_img}" resource="{page.gqueryCase}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gaestudio_img}" resource="{page.gaeCase}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.chosen_img}" resource="{page.chosenCase}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.jukito_img}" resource="{page.jukitoCase}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gsss_img}" resource="{page.gsssCase}"/>

                <widget:PricePresenter ui:field="casePrice"/>
            </div>
            <div class="{page.style.itemForSale}">
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.arcbees_img}" resource="{page.arcbeesUsb}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gwtp_img}" resource="{page.gwtpUsb}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gquery_img}" resource="{page.gqueryUsb}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gaestudio_img}" resource="{page.gaeUsb}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.chosen_img}" resource="{page.chosenUsb}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.jukito_img}" resource="{page.jukitoUsb}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gsss_img}" resource="{page.gsssUsb}"/>

                <widget:PricePresenter ui:field="keyPrice"/>
            </div>
            <div class="{page.style.itemForSale}">
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.arcbees_img}" resource="{page.arcbeesCup}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gwtp_img}" resource="{page.gwtpCup}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gquery_img}" resource="{page.gqueryCup}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gaestudio_img}" resource="{page.gaeCup}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.chosen_img}" resource="{page.chosenCup}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.jukito_img}" resource="{page.jukitoCup}"/>
                <g:Image addStyleNames="{page.style.itemForSale_img} {res.style.gsss_img}" resource="{page.gsssCup}"/>

                <widget:PricePresenter ui:field="mugPrice"/>
            </div>
        </section>

    </g:HTMLPanel>
</ui:UiBinder>
