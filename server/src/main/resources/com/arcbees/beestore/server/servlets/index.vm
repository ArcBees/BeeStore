##* @vtlvariable name="seo" type="java.util.ResourceBundle" *#
#set($title = ${seo.getString("title")})
#set($description = ${seo.getString("description")})
#set($keywords = ${seo.getString("keywords")})
#set($image = ${seo.getString("image")})
#set($image = ${seo.getString("imageWidth")})
#set($image = ${seo.getString("imageHeight")})
<!doctype html>
<html lang="$locale">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="gwt:property" content="locale=$locale">

    <title>$title</title>

    <meta property="description" content="$description">
    <meta property="og:title" content="$title">
    <meta property="og:description" content="$description">
    <meta property="og:image:width" content="$imageWidth">
    <meta property="og:image:height" content="$imageHeight">
    <meta property="og:image" content="$image">
    <meta property="og:type" content="website">
    <meta property="keywords" content="$keywords">

    <meta property="twitter:card" content="summary"/>
    <meta property="twitter:site" content="@Arcbees"/>
    <meta property="twitter:title" content="$title"/>
    <meta property="twitter:description" content="$description"/>
    <meta property="twitter:image" content="$image"/>

    <script type="text/javascript" src="/beestore/beestore.nocache.js"></script>

    <style type="text/css">
        #__gwt_historyFrame {
            position: absolute;
            width: 0;
            height: 0;
            border: 0;
        }

        noscript > div {
            width: 22em;
            position: absolute;
            left: 50%;
            margin-left: -11em;
            color: red;
            background-color: white;
            border: 1px solid red;
            padding: 4px;
            font-family: sans-serif;
        }
    </style>
</head>
<body>
<iframe src="javascript:''" id="__gwt_historyFrame" tabIndex='-1'></iframe>

<noscript>
    <div>
        Your web browser must have JavaScript enabled in order for this application to display correctly.
    </div>
</noscript>
</body>
</html>
